{
  "name": "oracle-plsql",
  "displayName": "oracle-plsql",
  "description": "Oracle PL/SQL syntax highlighting",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
    "homepage": "https://github.com/Zsapi/vsc-oracle-plsql",
    "repository": {
        "type": "git",
        "url": "https://github.com/Zsapi/vsc-oracle-plsql"
    },
    "icon": "images/oracle-plsql.png",
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
        "languages": [
            {
                "id": "oracle",
                "aliases": [
                    "PL/SQL (Oracle)",
                    "oracle"
                ],
                "extensions": [
                    ".sql",
                    ".ddl",
                    ".dml",
                    ".pks",
                    ".pkb",
                    ".pck",
                    ".tst"
                ],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "oracle",
      "scopeName": "source.plsql.oracle",
      "path": "./syntaxes/oracle.tmLanguage.json"
    }]
  }
}
